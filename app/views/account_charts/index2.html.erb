    <link href="https://www.grapecity.com/dataviewsjs/demos/static/dataviews/gc.dataviews.core.min.css" rel="stylesheet" type="text/css" />
    <link href="https://www.grapecity.com/dataviewsjs/demos/static/dataviews/gc.dataviews.grid.min.css" rel="stylesheet" type="text/css" />
    <link href="styles.css" rel="stylesheet" type="text/css" />

    <script src="https://www.grapecity.com/dataviewsjs/demos/static/js/app-polyfills.min.js" type="text/javascript"></script>

    <script type="text/javascript">
      window.process = {
        env: {
          NODE_ENV: 'production',
          USE_NPM: false,
          USE_CDN: false,
          SITE_ROOT: 'https://www.grapecity.com/dataviewsjs/demos',
          FRAMEWORK: 'purejs',
          DVJS_LICENSE_KEY:
            'GrapeCity-Internal-Use-Only,GrapeCity,E195393772372914##',
          SJS_LICENSE_KEY:
            '*.grapecity.com,E613631884219496#',
        },
      };
    </script>

    <script src="https://www.grapecity.com/dataviewsjs/demos/static/dataviews/gc.dataviews.common.min.js" type="text/javascript"></script>
    <script src="https://www.grapecity.com/dataviewsjs/demos/static/dataviews/gc.dataviews.core.min.js" type="text/javascript"></script>
    <script src="https://www.grapecity.com/dataviewsjs/demos/static/dataviews/gc.dataviews.grid.min.js" type="text/javascript"></script>

    <script src="https://www.grapecity.com/dataviewsjs/demos/static/js/license.js" type="text/javascript"></script>
  </head>

  <body>
   

<p id="notice"><%= notice %></p>

<h1>Account Charts</h1>

<table>
  <thead>
    <tr>
      <th>Account name</th>
      <th>Type</th>
      <th>Category</th>
      <th>Class</th>
      <th>Balance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_charts.each do |account_chart| %>
      <tr>
        <td><%= account_chart.account_name %></td>
        <td><%= account_chart.type %></td>
        <td><%= account_chart.category %></td>
        <td><%= account_chart.class %></td>
        <td><%= account_chart.balance %></td>
        <td><%= link_to 'Show', account_chart %></td>
        <td><%= link_to 'Edit', edit_account_chart_path(account_chart) %></td>
        <td><%= link_to 'Destroy', account_chart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account Chart', new_account_chart_path %>


  </body>
</html>
